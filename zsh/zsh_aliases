# SSH
alias pubkey="cat ~/.ssh/id_rsa.pub ; cat ~/.ssh/id_rsa.pub | pbcopy"

# Development shortcuts
alias g='git'
alias a='php artisan'
alias pf='phpunit --filter'

# Sysadmin shortcuts
alias dig='dig +short +noshort'

# Directories
alias la='cd /Users/mattias/Projects/Webdevelopment/ohdear.app'
